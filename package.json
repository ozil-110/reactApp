{
  "name": "rudex",
  "version": "1.0.0",
  "description": "reactApp",
  "main": "webpack.config.js",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run start-webpack-dev-server",
    "run-dev-server": "better-npm-run start-node-dev-server",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-webpack-dev-server": {
      "command": "node ./webpack/run-webpack-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-node-dev-server": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack/webpack.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "redux",
    "react",
    "ssr"
  ],
  "author": "liulang",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "less-loader": "^2.2.3",
    "morgan": "^1.8.1",
    "node-sass": "^4.4.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^2.0.0",
    "redux-devtools-log-monitor": "^1.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "antd": "^2.7.0",
    "babel-register": "^6.24.0",
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "moment": "^2.17.1",
    "mongoose": "^4.6.5",
    "qs": "^6.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.4.1",
    "webpack-isomorphic-tools": "^2.6.6"
  }
}
